	keywords = {Esophagus, Esophagectomy, Boerhaave’s syndrome, Esophageal perforations},
	file = {Texto completo:/Users/Ruana/Zotero/storage/P4GATQUW/Kaman et al. - 2010 - Management of Esophageal Perforation in Adults.pdf:application/pdf}
