@article{iuamoto_necrose_2015,
	title = {Necrose esofágica aguda secundária a endoprótese: relato de caso de primeiro sobrevivente},
	volume = {94},
	issn = {1679-9836, 0034-8554},
	url = {http://www.revistas.usp.br/revistadc/article/view/103740},
	doi = {10.11606/issn.1679-9836.v94i1p29-35},
	shorttitle = {Necrose esofágica aguda secundária a endoprótese},
	abstract = {Acute esophageal necrosis is one of the most serious complications associated with endovascular treatment of the descending aorta with stent placement. Its occurrence is rare, with only three cases reported in the literature. However, all this complication proved fatal in all cases. This paper describes, as far as we know, the first case of acute esophageal necrosis secondary to aortic stent graft in which the patient survives. 53 year old male patient with aortic dissection from the left subclavian artery to the bifurcation of the iliac arteries. Patient was submitted to endovascular stent treatment. The patient evolved with ischemic lesion of the middle and distal esophagus and total esophagectomy with gastrostomy and esophagostomy was performed for treatment. After a year and two months, patient underwent esophagogastroplasty for reconstruction of the gastrointestinal tract, with transposition of retrosternal gastric tube, from which remains virtually asymptomatic. The present case report is the first one in which the patient survived a esophagus necrosis secondary to endovascular stent treatment for thoracic aorta dissection.},
	pages = {29},
	number = {1},
	journaltitle = {Revista de Medicina},
	shortjournal = {Rev. Med. (São Paulo)},
	author = {Iuamoto, Leandro Ryuchi and Chikami, Fábio Hideki and Imakuma, Ernesto Sasaki and Collet e Silva, Francisco De Salles},
	urldate = {2020-03-28},
	date = {2015-03-05},
	langid = {portuguese},
